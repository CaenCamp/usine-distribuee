{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2019
  },
  "extends": ["plugin:prettier/recommended"],
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": ["error", "unix"],
    "prefer-expect-assertions": 0,
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
